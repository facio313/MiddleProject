package list.dao;

import java.util.List;

import common.QueryDao;
import list.vo.ListVO;

public class ShowListDao extends QueryDao implements IShowListDao{
	
	private static IShowListDao dao;
	private ShowListDao() {
	}
	public static IShowListDao getInstance() {
		if (dao == null) {
			dao = new ShowListDao();
		}
		return dao;
	}
	
	
	
	
	@Override
	public List<ListVO> showList() {
		
		return selectList("list.showList", null);
	}

}
