package review.dao;

import java.util.List;

import common.QueryDao;
import list.vo.ListVO;

public class ShowReviewDao extends QueryDao implements IShowReviewDao{
	
	private static IShowReviewDao dao;
	private ShowReviewDao() {
	}
	public static IShowReviewDao getInstance() {
		if (dao == null) {
			dao = new ShowReviewDao();
		}
		return dao;
	}
	@Override
	public List<ListVO> showReview() {
		
		return selectList("volunteer.showList", null);
	}

}
