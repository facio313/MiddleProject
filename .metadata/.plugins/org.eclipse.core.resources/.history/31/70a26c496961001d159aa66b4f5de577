package review.service;

import java.util.List;

import list.dao.IShowListDao;
import list.dao.ShowListDao;
import list.service.IShowListService;
import list.service.ShowListService;
import list.vo.ListVO;

public class ShowReviewService implements IShowReviewService{

	private IShowListDao dao;
	private static IShowListService service;
	private ShowListService() {
		dao = ShowListDao.getInstance();
	}
	public static IShowListService getInstance() {
		if (service == null) {
			service = new ShowListService();
		}
		return service;
	}
	
	@Override
	public List<ListVO> showList() {
		
		List<ListVO> volList = dao.showList();
		
		return volList;
	}

}
